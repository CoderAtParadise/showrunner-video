{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "dist",
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "isolatedModules": false,
    "esModuleInterop": true,
    "baseUrl": "./src",
    "noEmit": false,
    "noImplicitAny": false
  },
  "include": ["next-env.d.ts", "src/server/*.ts"]
}
